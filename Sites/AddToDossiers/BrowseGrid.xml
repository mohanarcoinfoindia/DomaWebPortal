<?xml version="1.0" encoding="utf-8" ?>
<screendef xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="..\Xsd\Grid.xsd">
  <mode>0</mode>
  <ImageListColumnCount>3</ImageListColumnCount>
  <ImageListOrientation>0</ImageListOrientation>
  <DefaultOrderBy>case DM_OBJECT.OBJ_TYPE when 'Folder' then 1 when 'Dossier' then 2 else 99 end, OBJ_UNAME ASC, OBJ_ID DESC</DefaultOrderBy>
  <DefaultFileOrderBy>FILE_TITLE ASC</DefaultFileOrderBy>
  <KeepSearchScreenVisible>false</KeepSearchScreenVisible>
  <FolderText>false</FolderText>
  <FiltersFile></FiltersFile>
  <GroupingFile></GroupingFile>
  <ToolbarFile></ToolbarFile>
  <ShowQueries>false</ShowQueries>
  <ShowSearchedFor>false</ShowSearchedFor>
  <ItemContextMenus>false</ItemContextMenus>
  <HeaderContextMenus>false</HeaderContextMenus>
  <columns>
   
    <column name="status" />
    <column name="expand" ></column>
    <column name="documentlinks" size="80" ShowToolTip="1" extra="cellpadding='0'"/>
    <column  name="title" chars="100" extra="NOWRAP" labelE="Title" labelN="Titel" labelF="Titre" ></column>
    <column name="version"></column>
    <column name="category" size="100" hidefilter="n"  ></column>
    <column name="creationdate"></column>
   
  </columns>

 </screendef>


