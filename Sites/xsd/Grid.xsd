                      <xs:enumeration value="checkoutend"/>
                      <xs:enumeration value="checkoutby"/>
                      <xs:enumeration value="checkoutcomment"/>
                      <xs:enumeration value="checkincomment"/>
                      <xs:enumeration value="linktype"/>
                      <xs:enumeration value="property"/>                      
                      <xs:enumeration value="constant"/>
                      <xs:enumeration value="folder"/>
                      <xs:enumeration value="mainfile"/>
                      <xs:enumeration value="file"/>
                      <xs:enumeration value="rank"/>
                      <xs:enumeration value="icon"/>
                      <xs:enumeration value="dragdrop"/>
                      <xs:enumeration value="name"/>
                      <xs:enumeration value="modifby"/>
                      <xs:enumeration value="modificationdate"/>
                      <xs:enumeration value="id"/>
                      <xs:enumeration value="din"/>
