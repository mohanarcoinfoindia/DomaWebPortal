                      <xs:enumeration value="lastnote"/>
                    </xs:restriction>
                  </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="assembly" type="xs:string" />
                <xs:attribute name="class" type="xs:string" />
                <xs:attribute name="formatter" type="xs:string" />
                <xs:attribute name="chars" type="xs:int" />
                <xs:attribute name="extra" type="xs:string" />
                <xs:attribute name="labelE" type="xs:string" />
                <xs:attribute name="labelN" type="xs:string" />
                <xs:attribute name="labelF" type="xs:string" />
                <xs:attribute name="filterwidth" type="xs:string" />
