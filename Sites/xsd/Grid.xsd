        <xs:element name="GroupingFile" type="xs:string" minOccurs="0" />
        <xs:element name="ToolbarFile" type="xs:string" minOccurs="0" />
        <xs:element name="ContextMenuFile" type="xs:string" minOccurs="0"/>
        <xs:element name="ForceJoinOnDMObject" type="xs:boolean" minOccurs="0" />
        <xs:element name="ShowQueries" type="xs:boolean" minOccurs="0" />
        <xs:element name="ShowSearchedFor" type="xs:boolean" minOccurs="0" />
        <xs:element name="AllowOrderBy" type="xs:boolean" minOccurs="0"  />
        <xs:element name="ShowFilters" type="xs:boolean" minOccurs="0"/>
        <xs:element name="ShowScroller" type="xs:boolean" minOccurs="0"/>
        <xs:element name="ShowSideBar" type="xs:boolean" minOccurs="0"/>
        <xs:element name="HeaderContextMenus" type="xs:boolean" minOccurs="0"/>
        <xs:element name="ItemContextMenus" type="xs:boolean" minOccurs="0"/>
        <xs:element name="AutoLoad" type="xs:boolean" minOccurs="0"/>
        <xs:element name="Category" type="xs:int" minOccurs="0"/>
        <xs:element name="Procedure" type="xs:int" minOccurs="0"/>
        <xs:element name="SelectionSite" type="xs:string" minOccurs="0"/>
        <xs:element name="RecordsPerPage" type="xs:int" minOccurs="0"/>
        <xs:element name="OpenDetailWindowsModal" type="xs:boolean" minOccurs="0"/>
        <xs:element name="EnableQuickUpload" type="xs:boolean" minOccurs="0"/>
        <xs:element ref="columns" minOccurs="0" maxOccurs="1" />              
        <xs:element name="rows" minOccurs="0" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="row" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element ref="columns" minOccurs="0" maxOccurs="unbounded" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>               
      </xs:all>
      
    
    </xs:complexType>
  </xs:element>
  <xs:element name="NewDataSet" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="1">        
        <xs:element ref="screendef" />
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>