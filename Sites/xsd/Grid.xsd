  <xs:element name="screendef">
    <xs:complexType>
      <xs:all>
        <xs:element name="mode" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:int">
              <xs:enumeration value="0"></xs:enumeration>
              <xs:enumeration value="1"></xs:enumeration>
            </xs:restriction>
            
          </xs:simpleType>
        </xs:element>
        <xs:element name="ImageListColumnCount" type="xs:int" minOccurs="0" />
        <xs:element name="ImageListOrientation" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:int">
              <xs:enumeration value="0"></xs:enumeration>
              <xs:enumeration value="1"></xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="ScrollerAlign" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="Center"></xs:enumeration>
              <xs:enumeration value="Left"></xs:enumeration>
              <xs:enumeration value="Right"></xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="DefaultOrderBy" type="xs:string" minOccurs="0" />
        <xs:element name="DefaultFileOrderBy" type="xs:string" minOccurs="0" />
        <xs:element name="KeepSearchScreenVisible" type="xs:boolean" minOccurs="0" />
        <xs:element name="FolderText" type="xs:boolean" minOccurs="0" />
