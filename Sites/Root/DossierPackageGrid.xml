  <ToolbarFile>DefaultDossiersPackageGridToolbar.xml</ToolbarFile>

  <AllowOrderBy>false</AllowOrderBy>
  <ShowFilters>false</ShowFilters>
  <ShowScroller>false</ShowScroller>
  <ShowSideBar>false</ShowSideBar>
  <HeaderContextMenus>false</HeaderContextMenus>
  <ItemContextMenus>true</ItemContextMenus>
  
  <columns>
    <column name="status" ></column>  
    <column name="title" chars="100" extra="NOWRAP" labelE="Title" labelN="Titel" labelF="Titre"></column>
    <column name="package"></column>
    <column name="category" size="100" hidefilter="n"></column>
    <column name="creationdate"></column>
    <column name="documentlinks" size="80"></column>        
  </columns>
</screendef>
